{
    "rewrites": [
        { "source": "/api/(.*)", "destination": "/api/$1" },
        { "source": "/(.*)", "destination": "/index.html" }
    ],
    "redirects": [
        {
          "source": "/app/(.*)",
          "destination": "https://studio.arachne.so/$1",
          "statusCode": 301
        }
    ],
    "scopes": {
        "studio.arachne.so": {
            "rewrites": [
                { "source": "/(.*)", "destination": "/app/$1" }
            ]
        }
    }
}
