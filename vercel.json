{
    "rewrites": [
        { "source": "/api/(.*)", "destination": "/api/$1" },
        { "source": "/(.*)", "destination": "/index.html" }
    ],
    "redirects": [
        {
          "source": "/app/(.*)",
          "destination": "https://studio.arachne.so/$1",
          "statusCode": 301
        },
        {
            "source": "/(.*)",
            "destination": "https://arachne.so/$1",
            "statusCode": 301
        }
      ]
}
